{
  "watch": [
    "src",
    "public",
    "prisma",
    "*.js",
    "*.json",
    "*.ts",
    "*.tsx"
  ],
  "ext": "js,jsx,ts,tsx,json,prisma,css,scss",
  "ignore": [
    "node_modules",
    ".next",
    "logs",
    "*.log",
    "dist",
    "build"
  ],
  "exec": "npm run dev",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 2000,
  "verbose": true,
  "restartable": "rs",
  "colours": true
}
